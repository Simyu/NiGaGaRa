<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="nigagara.goods">
	<insert id="insert" parameterType="goods">
		insert into goods values
		(goods_SEQ.nextval,#{goods_Name},#{weight},#{quantity},#{estimated_Price},#{receiver_Name},#{receiver_zipcode},#{receiver_Addr},#{receiver_Tel},#{sender_zipcode},#{sender_Addr},#{delivery_Tool},#{goods_Msg},'11','test')
	</insert>

</mapper>